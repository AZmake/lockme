/**布局**/
.flex {
  display: flex;
  width: 100%;
}

.flex.wrap {
  flex-wrap: wrap;
}

.main-start {
  justify-content: flex-start;
}

.main-center {
  justify-content: center;
}

.main-around {
  justify-content: space-around;
}

.cross-center {
  align-items: center;
}
/**布局**/

/**边距**/
.margin-top {
  margin-top: 20rpx;
}

.margin-right {
  margin-right: 20rpx;
}

.margin-bottom {
  margin-bottom: 20rpx;
}

.margin-left {
  margin-left: 20rpx;
}

.full-height {
  position: absolute;
  min-height: 100%;
}
/**边距**/

/**图标字体**/
@font-face {
  font-family: 'iconfont';
  src: url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2');
  font-weight: normal;
  font-style: normal;
}

.iconfont {
  font-family:"iconfont" !important;
  font-size: 16rpx;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-edit:before { content: "\e649"; }

.icon-search:before { content: "\e65c"; }

.icon-settings:before { content: "\e68a"; }

.icon-delete:before { content: "\e6b4"; }

.icon-add:before { content: "\e6da"; }

.icon-copy:before { content: "\e706"; }
.icon-enter:before { content: "\e6f8"; }
/**图标字体**/

/**按钮**/
.my-button {
  height: 100rpx;
  font-size: 32rpx;

  display: flex;
  justify-content: center;
  align-items: center;
}

.my-button.full {
  width: 100%;
}

.theme-black .my-button {
  color: #fff;
  background: #B4373A;
}

.theme-white .my-button {
  color: #fff;
  background: #000;
}

.my-button.default {
  color: #000;
  background: #F8F8F8;
}
/**按钮**/
